        ["Jun", 35, "blue"],
        ["Jul", 39, "red"],
        ["Aug", 32, "green"],
        ["Sep", 37, "blue"],
        ["Oct", 37, "red"],
        ["Nov", 33, "green"],
        ["Dec", 39, "blue"],
      ]);

      var view = new google.visualization.DataView(data);
      view.setColumns([0, 1,
                       { calc: "stringify",
                         sourceColumn: 1,
                         type: "string",
                         role: "annotation" },
                       2]);

      var options = {
        title: "#Accidents per Month",
        width: 500,
        height: 400,
        is3D: true,
        bar: {groupWidth: "95%"},
        legend: { position: "none" },
      };
      var chart = new google.visualization.ColumnChart(document.getElementById("hero-graph"));
      chart.draw(view, options);
  }

</script>

